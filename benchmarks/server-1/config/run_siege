#!/bin/bash
echo '==== Run 1 ====' > raw_results.txt
echo '$ siege -c 5 -b -r 100' >> raw_results.txt
uptime >> raw_results.txt
{ time siege -R siegerc -c 5 -b -r 100 -f ./urls.txt ; } 2>> raw_results.txt

echo '' >> raw_results.txt
echo '==== Run 2 ====' >> raw_results.txt
echo '$ siege -c 10 -b -r 50' >> raw_results.txt
uptime >> raw_results.txt
{ time siege -R siegerc -c 10 -b -r 50 -f ./urls.txt ; } 2>> raw_results.txt

echo '' >> raw_results.txt
echo '==== Run 3 ====' >> raw_results.txt
echo '$ siege -c 20 -b -r 25' >> raw_results.txt
uptime >> raw_results.txt
{ time siege -R siegerc -c 20 -b -r 25 -f ./urls.txt ; } 2>> raw_results.txt

